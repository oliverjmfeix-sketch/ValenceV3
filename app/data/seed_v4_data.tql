# ═══════════════════════════════════════════════════════════════════════════════
# BLOCKER EXCEPTION TYPES
# ═══════════════════════════════════════════════════════════════════════════════

insert $ex1 isa nonexclusive_license_exception, has exception_id "nonexclusive_license", has exception_name "Non-Exclusive License";
insert $ex2 isa ordinary_course_exception, has exception_id "ordinary_course", has exception_name "Ordinary Course of Business";
insert $ex3 isa intercompany_exception, has exception_id "intercompany", has exception_name "Intercompany Transfers";
insert $ex4 isa fair_value_exception, has exception_id "fair_value", has exception_name "Fair Value Sale";
insert $ex5 isa license_back_exception, has exception_id "license_back", has exception_name "License-Back Arrangement";
insert $ex6 isa immaterial_ip_exception, has exception_id "immaterial_ip", has exception_name "Immaterial IP";

# ═══════════════════════════════════════════════════════════════════════════════
# SWEEP EXEMPTION TYPES
# ═══════════════════════════════════════════════════════════════════════════════

insert $se1 isa non_collateral_exemption, has exemption_id "non_collateral", has exemption_name "Non-Collateral Assets";
insert $se2 isa ordinary_course_sale_exemption, has exemption_id "ordinary_course_sale", has exemption_name "Ordinary Course Sales";
insert $se3 isa casualty_exemption, has exemption_id "casualty", has exemption_name "Casualty Events";
insert $se4 isa below_threshold_exemption, has exemption_id "below_threshold", has exemption_name "Below De Minimis Threshold";
insert $se5 isa ratio_basket_exemption, has exemption_id "ratio_basket", has exemption_name "Ratio Basket Sales";
